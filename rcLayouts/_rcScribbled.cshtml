<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>@Page.mTitle</title>
<meta name="keywords" content="@Page.mKeywords" />
<meta name="description" content="@Page.mDescription" />
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<link href="@Href("~/rcLayouts/scribbled/style.css")" rel="stylesheet" type="text/css" media="screen" />
@RenderSection("head")
</head>
<body>
<div id="wrapper">
	<div id="wrapper-bgbtm">
		<div id="bg1">
			<div id="bg2">
				<div id="bg3">
					<div id="header">
						<div id="logo">
							@RenderSection("rcTop",false)
						</div>
					</div>
					<!-- end #header -->
					<div id="menu">
						@HelperMenu.SimpleMenu()
					</div>
					<!-- end #menu -->
					<div id="page">
						<div id="page-bgtop">
							<div id="page-bgbtm">
								<div id="content">
									@RenderBody()
                                    @RenderSection("rcBottom",false)
								</div>
								<!-- end #content -->
								<div id="sidebar">
									<ul>
                                    @RenderSection("rcLeft",false)
                                    @RenderSection("rcRight",false)
									</ul>
								</div>
								<!-- end #sidebar -->
								<div style="clear: both;">&nbsp;</div>
							</div>
						</div>
					</div>
					<!-- end #page -->
				</div>
			</div>
		</div>
		<div id="footer">
			<p>Copyright (c) @DateTime.Now.Year Sitename.com. All rights reserved. Design by <a href="http://www.freecsstemplates.org/"> CSS Templates</a>.</p>
		</div>
		<!-- end #footer -->
	</div>
</div>
</body>
</html>
